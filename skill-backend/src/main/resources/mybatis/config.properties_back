# JDBC 驱动类名
jdbc.driverClassName=com.mysql.jdbc.Driver

# JDBC URL: jdbc:mysql:// + 数据库主机地址 + 端口号 + 数据库名
jdbc.url=jdbc:mysql://118.xxx.xxx.xxx:3306/kaoqin?serverTimezone=UTC&useSSL=true

# JDBC 用户名及密码
jdbc.username=xxx
jdbc.password=xxx

# 生成实体类所在的包
package.model=cn.hqweay.skill.entity

# 生成 mapper 类所在的包
package.mapper=cn.hqweay.skill.dao

# 生成 mapper xml 文件所在的包，默认存储在 resources 目录下
package.xml=mybatis/mapper
